{
  "dt": {
    "$resolver": "timestamp",
    "pattern": {
      "format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'",
      "timeZone": "UTC"
    }
  },
  "exception": {
    "exception_class": {
      "$resolver": "exception",
      "field": "className"
    },
    "exception_message": {
      "$resolver": "exception",
      "field": "message"
    },
    "stacktrace": {
      "$resolver": "exception",
      "field": "stackTrace",
      "stackTrace": {
        "stringified": true
      }
    }
  },
  "level": {
    "$resolver": "level",
    "field": "name"
  },
  "message": {
    "$resolver": "message",
    "stringified": true
  },
  "line_number": {
    "$resolver": "source",
    "field": "lineNumber"
  },
  "thread_name": {
    "$resolver": "thread",
    "field": "name"
  },
  "service": {
    "$resolver": "pattern",
    "pattern": "conversation-manager",
    "stackTraceEnabled": false
  },
  "hostname": "${hostName}",
  "tenant": {
    "$resolver": "pattern",
    "pattern": "%X{tenant}",
    "stackTraceEnabled": false
  },
  "handoff_id": {
    "$resolver": "pattern",
    "pattern": "%X{handoff_id}",
    "stackTraceEnabled": false
  },
  "routing_iteration": {
    "$resolver": "pattern",
    "pattern": "%X{routing_iteration}",
    "stackTraceEnabled": false
  },
  "trace_id": {
    "$resolver": "pattern",
    "pattern": "%X{traceId}",
    "stackTraceEnabled": false
  },
  "span_id": {
    "$resolver": "pattern",
    "pattern": "%X{spanId}",
    "stackTraceEnabled": false
  },
  "conversation_id": {
    "$resolver": "pattern",
    "pattern": "%X{conversation_id}",
    "stackTraceEnabled": false
  },
  "class": {
    "$resolver": "logger",
    "field": "name"
  }
}